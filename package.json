{
  "name": "my-online-store",
  "version": "1.0.0",
  "description": "express and react app",
  "main": "app.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production&& node app.js",
    "server": "nodemon app.js",
    "client": "npm run start --prefix client",
    "client:install": "npm install --prefix client",
    "client:build": "npm run build --prefix client",
    "build": "cross-env NODE_ENV=production&& concurrently \" npm start \" \" npm run client:build \"",
    "dev": "cross-env NODE_ENV=development concurrently \" npm run server \" \"npm run client \""
  },
  "keywords": [
    "react",
    "express"
  ],
  "author": "Helena Bali <elenabaltajs@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.2.1",
    "config": "^3.3.8",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "fs": "^0.0.1-security"
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.20"
  }
}
